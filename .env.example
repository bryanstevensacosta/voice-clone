# Environment Configuration Example
# Copy this file to .env and fill in your values

# Python Environment
PYTHON_VERSION=3.10

# Model Configuration
MODEL_NAME=Qwen/Qwen3-TTS-12Hz-1.7B-Base
QWEN_TTS_CACHE_DIR=./data/qwen3_models

# Audio Configuration
SAMPLE_RATE=12000
AUDIO_FORMAT=wav

# Output Configuration
OUTPUT_DIR=./data/outputs
SAMPLE_DIR=./data/samples

# Logging
LOG_LEVEL=INFO
LOG_FILE=./voice_clone.log

# Development
DEBUG=false
